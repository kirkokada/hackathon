<div class="row">
  <% @allentries.each do |e| %>
      <div class="col-md-10 col-md-offset-1 entry">
        <span class="title text-left"><%= link_to truncate(e.name, length: 30), entry_url(e) %></span>
        <a href="<%= e.url %>" class="repo-link"><span class="lead"><%= truncate(e.url, length: 35) %></span></a>
        <p class="lead text-left"><%= e.about %></p>
      </div>
  <% end %>
</div>
