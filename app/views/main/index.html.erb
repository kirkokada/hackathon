<div class="row">
  <% @allentries.each do |e| %>
      <div class="col-md-10 col-md-offset-1 entry">
        <span class="title text-left"><%= truncate(e.name, length: 33) %></span>
        <a href="<%= e.url %>"><span class="lead"><%= truncate(e.url, length: 45) %></span></a>
        <p class="lead text-left"><%= e.about %></p>
      </div>
  <% end %>
</div>
